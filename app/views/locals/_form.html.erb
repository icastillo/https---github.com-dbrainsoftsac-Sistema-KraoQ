<%= simple_form_for(@local) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :no_local %>
    <%= f.input :tx_direccion %>
    <%= f.input :tx_telefono %>
    <%= f.input :tx_correo %>
    <%= f.input :tx_descripcion %>
    <%= f.input :co_administrador %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
